#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/rrome/node_modules/.pnpm/convex@1.25.4_@auth0+auth0-_c9b7af4625f00012c3c396fec55bfd46/node_modules/convex/bin/node_modules:/mnt/c/Users/rrome/node_modules/.pnpm/convex@1.25.4_@auth0+auth0-_c9b7af4625f00012c3c396fec55bfd46/node_modules/convex/node_modules:/mnt/c/Users/rrome/node_modules/.pnpm/convex@1.25.4_@auth0+auth0-_c9b7af4625f00012c3c396fec55bfd46/node_modules:/mnt/c/Users/rrome/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/rrome/node_modules/.pnpm/convex@1.25.4_@auth0+auth0-_c9b7af4625f00012c3c396fec55bfd46/node_modules/convex/bin/node_modules:/mnt/c/Users/rrome/node_modules/.pnpm/convex@1.25.4_@auth0+auth0-_c9b7af4625f00012c3c396fec55bfd46/node_modules/convex/node_modules:/mnt/c/Users/rrome/node_modules/.pnpm/convex@1.25.4_@auth0+auth0-_c9b7af4625f00012c3c396fec55bfd46/node_modules:/mnt/c/Users/rrome/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/convex@1.25.4_@auth0+auth0-_c9b7af4625f00012c3c396fec55bfd46/node_modules/convex/bin/main.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/convex@1.25.4_@auth0+auth0-_c9b7af4625f00012c3c396fec55bfd46/node_modules/convex/bin/main.js" "$@"
fi
